<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.managementsite.mapper.member.UpdateMember">
<insert id="updateMember" parameterType="com.test.managementsite.data.UpdateMemberForm">
		UPDATE  
			member
		SET
			sex = #{sex},
			address = #{address},
			age = #{age},
			phone = #{phone}
		WHERE
			name = #{name}
</insert>
</mapper>