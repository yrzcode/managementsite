<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.managementsite.mapper.member.InsertMember">
<insert id="insertMember" parameterType="com.test.managementsite.data.MemberForm">
		insert into member
			 (name,sex,address,age,phone)
		values(
			 #{name}, #{sex}, #{address},#{age},#{phone})
</insert>
</mapper>